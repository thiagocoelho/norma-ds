  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "^5.0.0-alpha.136",
    "@mui/material": "^5.14.0",
    "@mui/styles": "^5.14.0",
    "@svgr/core": "^5.3.1",
    "@svgr/plugin-jsx": "^5.3.1",
    "@svgr/plugin-prettier": "^5.3.1",
    "@svgr/plugin-svgo": "^5.3.0",
    "autoprefixer": "^9.5.1",
    "camelcase": "^6.0.0",
    "chalk": "4.1.2",
    "colors": "^1.4.0",
    "fs": "^0.0.1-security",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "storybook-color-picker": "^3.1.0",
    "web-vitals": "^2.1.4",
    "xml2js": "^0.6.0"
  },
  "scripts": {
    "start": "react-scripts start --no-manager-cache -p 7000",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "icons": "node ./scripts/generate-svg-icons.js",
    "extract-svg": "node ./scripts/extractSVG.js",
    "storybook": "storybook dev -p 6007 -s public",
    "build-storybook": "storybook build -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.1.0",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-mdx-gfm": "^7.1.0",
    "@storybook/node-logger": "^7.1.0",
    "@storybook/preset-create-react-app": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-webpack5": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.1.0",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "babel-plugin-named-exports-order": "^0.0.2",
    "prop-types": "^15.8.1",
    "storybook": "^7.1.0",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oloslive/norma-ds.git"
  }
